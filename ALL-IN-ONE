-- Buat Tabs dan Buttons otomatis
for _, tabData in ipairs(scriptList) do
    local tab = Window:Tab({ Title = tabData.TabName, Icon = "code" })
    for _, script in ipairs(tabData.Scripts) do
        tab:Button({
            Title = script.Title,
            Callback = function()
                local success, err = pcall(function()
                    loadstring(game:HttpGet(script.Url))()
                end)
                if success then
                    WindUI:Notify({
                        Title = "Success",
                        Content = script.Title.." Executed!",
                        Icon = "bell",
                        Duration = 3,
                    })
                else
                    WindUI:Notify({
                        Title = "Error",
                        Content = "Failed to load: "..tostring(err),
                        Icon = "bell",
                        Duration = 5,
                    })
                end
            end
        })
    end
end
